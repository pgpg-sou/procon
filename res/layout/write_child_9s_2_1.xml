                 android:layout_width="wrap_content" 
                 android:text="----"></TextView>
                
		</LinearLayout>



		<!-- 引き続き観察を要する事項 -->
		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			style="@style/write_9_question_box">

			<TextView
				android:text="@string/write_9s_1_item14"
				android:id="@+id/write_9s_1_item14"
				style="@style/write_9_question"/>
			
			<!-- 引き続き観察を要する事項 -->
                <TextView style="@style/blackOnly"  
                 android:id="@+id/write_child_9s_2_07" 
                 android:layout_height="wrap_content" 
                 android:layout_width="wrap_content" 
                 android:text="----"></TextView>
                
		</LinearLayout>


		<!-- 施設名または担当者名 -->
		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			style="@style/write_9_question_box">

			<TextView
				android:text="@string/write_9s_charge"
				android:id="@+id/write_9s_charge"
				style="@style/write_9_question"/>
			
			<!-- 施設名または担当者名 -->
                <TextView style="@style/blackOnly"  
                 android:id="@+id/write_child_9s_2_08" 
                 android:layout_height="wrap_content" 
                 android:layout_width="wrap_content" 
                 android:text="----"></TextView>
                
		</LinearLayout>

		<!-- 電話 -->
		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			style="@style/write_9_question_box">

			<TextView
				android:text="@string/write_9s_1_item15"
				android:id="@+id/write_9s_1_item15"
				style="@style/write_9_question"/>
			
			<!-- 電話 -->
                <TextView style="@style/blackOnly"  
                 android:id="@+id/write_child_9s_2_09" 
                 android:layout_height="wrap_content" 
                 android:layout_width="wrap_content" 
                 android:text="----"></TextView>
		</LinearLayout>

	</LinearLayout>

</ScrollView>

</LinearLayout>
