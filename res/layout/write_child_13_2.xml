<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent" android:orientation="vertical" android:background="@drawable/background_pink">
  <!-- 読込・保存ボタン -->
    <TextView  android:id="@+id/TextView01" style="@style/write_9_title" android:text="保護者の記録　7歳以降"></TextView>
    <LinearLayout
         android:layout_height="wrap_content" 
         android:id="@+id/linearLayout8" 
         android:layout_width="match_parent" 
         style="@style/write_9_question_box">
        

        <Button 
            android:layout_height="wrap_content" 
            android:id="@+id/Button_Write_child_13_2_back" 
            android:layout_width="wrap_content" 
            android:layout_weight="1" 
            android:text="編集する"></Button>
        
        <Button 
            android:layout_height="wrap_content" 
            android:id="@+id/Button_Write_child_13_2_home" 
            android:layout_width="wrap_content"
             android:layout_weight="1"
              android:text="編集終了"></Button>
        
        <Button 
            android:layout_height="wrap_content" 
            android:id="@+id/Button_Write_child_13_2_next" 
            android:layout_width="wrap_content" 
            android:layout_weight="1"
             android:text="キャンセル"></Button>
    </LinearLayout>
    <ScrollView android:layout_height="wrap_content" android:id="@+id/scrollView1" android:layout_width="fill_parent" android:layout_gravity="top">
        <LinearLayout android:layout_width="fill_parent" android:layout_height="wrap_content" android:orientation="vertical" android:id="@+id/linearLayout4">
            <TextView  style="@style/write_9_title" android:id="@+id/textView1" android:text="身長・体重"></TextView>
            <LinearLayout android:layout_height="wrap_content" android:orientation="vertical" android:id="@+id/LinearLayout01" android:layout_width="fill_parent" style="@style/write_9_question_box">
                <HorizontalScrollView android:id="@+id/horizontalScrollView1" android:layout_height="wrap_content" android:layout_width="wrap_content">
                    <LinearLayout android:layout_width="fill_parent" android:layout_height="fill_parent" android:orientation="horizontal" android:id="@+id/linearLayout3">
                        <TableLayout android:id="@+id/tableLayout1" android:layout_height="wrap_content" android:layout_width="fill_parent">
                            <TableRow android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/tableRow2">
                                <TextView style="@style/blackOnly"  android:id="@+id/TextView05" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="年月日"></TextView>
                                <TextView style="@style/blackOnly"  android:id="@+id/TextView13" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="年齢"></TextView>
                                <TextView style="@style/blackOnly"  android:id="@+id/TextView10" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="体重[kg]"></TextView>
                                <TextView style="@style/blackOnly"  android:id="@+id/TextView14" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="身長[cm]"></TextView>
                                <TextView style="@style/blackOnly"  android:id="@+id/TextView16" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="その他"></TextView>
                            </TableRow>
                            <TableRow android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/tableRow1">
                                <LinearLayout android:layout_height="wrap_content" android:id="@+id/LinearLayout02" android:layout_width="wrap_content">
                                    <EditText android:id="@+id/EditText_write_child_13_2__write_child_13_2_01" 
                                        android:layout_height="wrap_content" 
                                        android:layout_width="wrap_content" 
                                        android:text="@+id/EditText_write_child_13_2__write_child_13_2_01"></EditText>
                                    
                                    <TextView style="@style/blackOnly"  android:id="@+id/TextView08" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="@string/write_1_year"></TextView>
                                    <EditText android:id="@+id/EditText_write_child_13_2__write_child_13_2_02" 
                                        android:layout_height="wrap_content" 
                                        android:layout_width="wrap_content" 
                                        android:text="@+id/EditText_write_child_13_2__write_child_13_2_02"></EditText>
                                    
                                    <TextView style="@style/blackOnly"  android:id="@+id/TextView08" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="@string/write_1_month"></TextView>
                                    <EditText android:id="@+id/EditText_write_child_13_2__write_child_13_2_03" 
                                        android:layout_height="wrap_content" 
                                        android:layout_width="wrap_content" 
                                        android:text="@+id/EditText_write_child_13_2__write_child_13_2_03"></EditText>
                                    
                                    <TextView style="@style/blackOnly"  android:id="@+id/TextView07" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="日"></TextView>
                                </LinearLayout>
                                <LinearLayout android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/LinearLayout07">
                                    <EditText android:id="@+id/EditText_write_child_13_2__write_child_13_2_04" 
                                        android:layout_height="wrap_content" 
                                        android:layout_width="wrap_content" 
                                        android:text="@+id/EditText_write_child_13_2__write_child_13_2_04"></EditText>
                                    
                                    <TextView style="@style/blackOnly"  android:layout_width="wrap_content" android:id="@+id/TextView20" android:layout_height="wrap_content" android:text="歳"></TextView>
                                </LinearLayout>
                                <LinearLayout android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/LinearLayout08">
                                    <EditText android:layout_width="wrap_content" 
                                        android:id="@+id/EditText_write_child_13_2__write_child_13_2_05" 
                                        android:layout_height="wrap_content" 
                                        android:text="@+id/EditText_write_child_13_2__write_child_13_2_05"></EditText>
                                    
                                    <TextView style="@style/blackOnly"  android:layout_width="wrap_content" android:id="@+id/TextView19" android:layout_height="wrap_content" android:text="kg"></TextView>
                                </LinearLayout>
                                <LinearLayout android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/LinearLayout09">
                                    <EditText android:layout_width="wrap_content" android:id="@+id/EditText_write_child_13_2_06" android:layout_height="wrap_content" android:text="@+id/EditText_write_child_13_2_06"></EditText>
                                    <TextView style="@style/blackOnly"  android:layout_width="wrap_content" android:id="@+id/TextView21" android:layout_height="wrap_content" android:text="cm"></TextView>
                                </LinearLayout>
                                <EditText android:layout_width="wrap_content" android:id="@+id/EditText_write_child_13_2_07" android:layout_height="wrap_content" android:text="@+id/EditText_write_child_13_2_07"></EditText>
                            </TableRow>
                        </TableLayout>
                    </LinearLayout>
                </HorizontalScrollView>
            </LinearLayout>
            <TextView  android:id="@+id/TextView15" android:layout_height="wrap_content" android:layout_width="wrap_content" style="@style/write_9_title" android:text="予防接種の記録"></TextView>
            <LinearLayout style="@style/write_9_question_box" android:layout_height="wrap_content" android:orientation="vertical" android:layout_width="fill_parent" android:id="@+id/LinearLayout05">
                <HorizontalScrollView android:id="@+id/HorizontalScrollView01" android:layout_height="wrap_content" android:layout_width="wrap_content">
                    <LinearLayout android:layout_width="fill_parent" android:layout_height="fill_parent" android:orientation="horizontal" android:id="@+id/LinearLayout03">
                        <TableLayout android:id="@+id/TableLayout01" android:layout_height="wrap_content" android:layout_width="fill_parent">
                            <TableRow android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/TableRow01">
                                <TextView style="@style/blackOnly"  android:id="@+id/TextView06" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="予防接種の種類"></TextView>
                                <TextView style="@style/blackOnly"  android:id="@+id/TextView11" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="接種年月日"></TextView>
                                <TextView style="@style/blackOnly"  android:id="@+id/TextView12" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="備考"></TextView>
                            </TableRow>
                            <TableRow android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/TableRow02">
                                <TextView style="@style/blackOnly"  android:id="@+id/TextView04" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="ジフテリア・破傷風(第2期)"></TextView>
                                <LinearLayout android:layout_height="wrap_content" android:id="@+id/LinearLayout04" android:layout_width="wrap_content">
                                    <EditText android:id="@+id/EditText_write_child_13_2_08" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="@+id/EditText_write_child_13_2_08"></EditText>
                                    <TextView style="@style/blackOnly"  android:id="@+id/TextView02" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="@string/write_1_year"></TextView>
                                    <EditText android:id="@+id/EditText_write_child_13_2_09" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="@+id/EditText_write_child_13_2_09"></EditText>
                                    <TextView style="@style/blackOnly"  android:id="@+id/TextView02" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="@string/write_1_month"></TextView>
                                    <EditText android:id="@+id/EditText_write_child_13_2_10" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="@+id/EditText_write_child_13_2_10"></EditText>
                                    <TextView style="@style/blackOnly"  android:id="@+id/TextView03" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="日"></TextView>
                                </LinearLayout>
                                <EditText android:id="@+id/EditText_write_child_13_2_11" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@+id/EditText_write_child_13_2_11"></EditText>
                            </TableRow>
                            <TableRow android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/TableRow03">
                                <TextView style="@style/blackOnly"  android:id="@+id/TextView17" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="日本脳炎(第2期)"></TextView>
                                <LinearLayout android:layout_height="wrap_content" android:id="@+id/LinearLayout06" android:layout_width="wrap_content">
                                    <EditText android:id="@+id/EditText_write_child_13_2_12" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="@+id/EditText_write_child_13_2_12"></EditText>
                                    <TextView style="@style/blackOnly"  android:id="@+id/TextView09" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="@string/write_1_year"></TextView>
                                    <EditText android:id="@+id/EditText_write_child_13_2_13" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="@+id/EditText_write_child_13_2_13"></EditText>
                                    <TextView style="@style/blackOnly"  android:id="@+id/TextView09" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="@string/write_1_month"></TextView>
                                    <EditText android:id="@+id/EditText_write_child_13_2_14" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="@+id/EditText_write_child_13_2_14"></EditText>
                                    <TextView style="@style/blackOnly"  android:id="@+id/TextView18" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="日"></TextView>
                                </LinearLayout>
                                <EditText android:id="@+id/EditText_write_child_13_2_15" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@+id/EditText_write_child_13_2_15"></EditText>
                            </TableRow>
                        </TableLayout>
                    </LinearLayout>
                </HorizontalScrollView>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
<!-- 読込・保存ボタン -->

</LinearLayout>
