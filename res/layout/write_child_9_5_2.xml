<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:orientation="vertical" android:background="@drawable/background_pink">



    <TextView style="@style/write_9_title" android:layout_height="wrap_content" android:text="@string/write_9s_5_title" android:id="@+id/write_9s_5_title"></TextView>
<LinearLayout
             android:layout_height="wrap_content" 
             android:id="@+id/linearLayout8" 
             android:layout_width="match_parent" 
             style="@style/write_9_question_box">
             
        <Button 
            android:layout_height="wrap_content" 
            android:id="@+id/ButtonLock" 
            android:layout_width="wrap_content" 
            android:layout_weight="1" 
            android:text="編集する"></Button>
        
        <Button 
            android:layout_height="wrap_content"
             android:id="@+id/ButtonUnlock" 
             android:layout_width="wrap_content" 
             android:layout_weight="1" 
             android:text="編集終了"></Button>
        
        <Button 
            android:layout_height="wrap_content" 
            android:id="@+id/ButtonCancel" 
            android:layout_width="wrap_content" 
            android:layout_weight="1" 
            android:text="キャンセル"></Button>
    </LinearLayout>



<ScrollView
	android:id="@+id/ScrollView01"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content">

	<LinearLayout
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:orientation="vertical">

		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			style="@style/write_9_question_box">

			<LinearLayout
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="horizontal">
				
				<EditText android:id="@+id/EditText_write_child_9_5_2_01" 
				    android:layout_height="wrap_content" 
				    android:text="@+id/EditText_write_child_9_5_2_01" 
				    android:layout_width="wrap_content"></EditText>

				<TextView
					android:text="@string/write_year"
					android:id="@+id/write_year"
					style="@style/write_9_question"/>
				
				<EditText android:id="@+id/EditText_write_child_9_5_2_02" 
				    android:layout_height="wrap_content" 
				    android:text="@+id/EditText_write_child_9_5_2_02" 
				    android:layout_width="wrap_content"></EditText>

				<TextView
					android:text="@string/write_month"
					android:id="@+id/write_month"
					style="@style/write_9_question"/>
				
				

				<TextView
					android:text="@string/write_date"
					android:id="@+id/write_date"
					style="@style/write_9_question"/>

				<TextView
					android:text="@string/write_enforcement"
					android:id="@+id/write_enforcement"
					style="@style/write_9_question"/>

			</LinearLayout>


			<LinearLayout
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="horizontal">
				
				<EditText android:id="@+id/EditText_write_child_9_5_2_03" 
				    android:layout_height="wrap_content" 
				    android:text="@+id/EditText_write_child_9_5_2_03" 
				    android:layout_width="wrap_content"></EditText>

				<TextView
					android:text="@string/write_kamonth"
					android:id="@+id/write_kamonth"
					style="@style/write_9_question"/>
				
				<EditText android:id="@+id/EditText_write_child_9_5_2_04" 
				    android:layout_height="wrap_content" 
				    android:text="@+id/EditText_write_child_9_5_2_04" 
				    android:layout_width="wrap_content"></EditText>

				<TextView
					android:text="@string/write_date"
					android:id="@+id/write_date"
					style="@style/write_9_question"/>

			</LinearLayout>

		</LinearLayout>



		<!-- 体重 -->
		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			style="@style/write_9_question_box">

			<LinearLayout
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="horizontal">

				<TextView
					android:text="@string/write_weight"
					android:id="@+id/write_weight"
					style="@style/write_9_question"/>
				
				<EditText android:id="@+id/EditText_write_child_9_5_2_05" 
				    android:layout_height="wrap_content" 
				    android:text="@+id/EditText_write_child_9_5_2_05" 
				    android:layout_width="wrap_content"></EditText>

				<TextView
					android:text="@string/write_g"
					android:id="@+id/write_g"
					android:layout_width="wrap_content"
					style="@style/write_9_question"/>

			</LinearLayout>



			<!-- 身長 -->
			<LinearLayout
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="horizontal">

				<TextView
					android:text="@string/write_height"
					android:id="@+id/write_height"
					style="@style/write_9_question"/>
				
				<EditText android:id="@+id/EditText_write_child_9_5_2_06" 
				    android:layout_height="wrap_content" 
				    android:text="@+id/EditText_write_child_9_5_2_06" 
				    android:layout_width="wrap_content"></EditText>

				<TextView
					android:text="@string/write_cm"
					android:id="@+id/write_cm"
					android:layout_width="wrap_content"
					style="@style/write_9_question"/>

			</LinearLayout>



			<!-- 胸囲 -->
			<LinearLayout
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="horizontal">

				<TextView
					android:text="@string/write_chest"
					android:id="@+id/write_chest"
					style="@style/write_9_question"/>
				
				<EditText android:id="@+id/EditText_write_child_9_5_2_07" 
				    android:layout_height="wrap_content" 
				    android:text="@+id/EditText_write_child_9_5_2_07" 
				    android:layout_width="wrap_content"></EditText>

				<TextView
					android:text="@string/write_cm"
					android:id="@+id/write_cm"
					android:layout_width="wrap_content"
					style="@style/write_9_question"/>

			</LinearLayout>



			<!-- 頭囲 -->
			<LinearLayout
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="horizontal">

				<TextView
					android:text="@string/write_head"
					android:id="@+id/write_head"
					style="@style/write_9_question"/>
				
				<EditText android:id="@+id/EditText_write_child_9_5_2_08" 
				    android:layout_height="wrap_content" 
				    android:text="_@+id/EditText_write_child_9_5_2_08"
				    android:layout_width="wrap_content"></EditText>

				<TextView
					android:text="@string/write_cm"
					android:id="@+id/write_cm"
					android:layout_width="wrap_content"
					style="@style/write_9_question"/>

			</LinearLayout>

		</LinearLayout>



		<!-- 栄養状態 -->
		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			style="@style/write_9_question_box">

			<TextView
				android:text="@string/write_9s_nutrition"
				android:id="@+id/write_9s_nutrition"
				style="@style/write_9_question"/>
			<Spinner android:id="@+id/Spinner01" android:layout_width="fill_parent" android:layout_height="wrap_content"></Spinner>

		</LinearLayout>


		<!-- 栄養法 -->
		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			style="@style/write_9_question_box">

			<TextView
				android:id="@+id/write_9s_nutritionway"
				style="@style/write_9_question" android:text="離乳食"/>
			<LinearLayout android:id="@+id/LinearLayout06" android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
				<TextView style="@style/write_9_question" android:layout_height="wrap_content" android:id="@+id/TextView07" android:layout_width="wrap_content" android:text="1日"></TextView>
				
				<EditText android:id="@+id/EditText_write_child_9_5_2_09" 
				    android:layout_height="wrap_content" 
				    android:text="@+id/EditText_write_child_9_5_2_09" 
				    android:layout_width="wrap_content"></EditText>
				
				<TextView style="@style/write_9_question" android:layout_height="wrap_content" android:id="@+id/TextView06" android:layout_width="wrap_content" android:text="回"></TextView>
			</LinearLayout>

		</LinearLayout>



		<!-- 健康・要観察 -->
		<LinearLayout style="@style/write_9_question_box" android:id="@+id/LinearLayout05" android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="wrap_content">
			<TextView style="@style/write_9_question" android:layout_height="wrap_content" android:id="@+id/TextView02" android:layout_width="wrap_content" android:text="歯"></TextView>
			<LinearLayout android:id="@+id/linearLayout2" android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
				
				<EditText android:id="@+id/EditText_write_child_9_5_2_10" 
				    android:layout_height="wrap_content"
				    android:text="@+id/EditText_write_child_9_5_2_10" 
				    android:layout_width="wrap_content"></EditText>
				
				<TextView style="@style/write_9_question" android:layout_height="wrap_content" android:id="@+id/TextView03" android:layout_width="wrap_content" android:text="本"></TextView>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			style="@style/write_9_question_box">
			<TextView style="@style/write_9_question" android:layout_height="wrap_content" android:id="@+id/TextView04" android:layout_width="wrap_content" android:text="口の中の疾患や異常※"></TextView>
			<Spinner android:id="@+id/Spinner02" android:layout_width="fill_parent" android:layout_height="wrap_content"></Spinner>
			
			<EditText android:id="@+id/EditText_write_child_9_5_2_11" 
			    android:layout_height="wrap_content" 
			    android:text="@+id/EditText_write_child_9_5_2_11" 
			    android:layout_width="wrap_content"></EditText>
			
			
			<TextView style="@style/write_9_question" android:layout_height="wrap_content" android:id="@+id/TextView05" android:layout_width="wrap_content" android:text="※口の中の疾患や異常は，むし歯，歯ぐきの病気，不正咬合などを含みます。"></TextView>

		</LinearLayout>


		<!-- 指導事項 -->
		<LinearLayout style="@style/write_9_question_box" android:id="@+id/linearLayout3" android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="wrap_content">
			<Spinner android:id="@+id/Spinner03" android:layout_width="fill_parent" android:layout_height="wrap_content"></Spinner>
			
			<EditText android:id="@+id/EditText_write_child_9_5_2_12" 
			    android:layout_height="wrap_content"
			    android:text="@+id/EditText_write_child_9_5_2_12" 
			    android:layout_width="wrap_content"></EditText>
			
		</LinearLayout>
		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			style="@style/write_9_question_box">

			<TextView
				android:text="@string/write_9s_coaching"
				android:id="@+id/write_9s_coaching"
				style="@style/write_9_question"/>
			
			<EditText android:id="@+id/EditText_write_child_9_5_2_13" 
			    android:layout_height="wrap_content" 
			    android:text="@+id/EditText_write_child_9_5_2_13" 
			    android:layout_width="wrap_content"></EditText>

		</LinearLayout>


		<!-- 施設名または担当者名 -->
		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			style="@style/write_9_question_box">

			<TextView
				android:text="@string/write_9s_charge"
				android:id="@+id/write_9s_charge"
				style="@style/write_9_question"/>
			
			<EditText android:id="@+id/EditText_write_child_9_5_2_14" 
			    android:layout_height="wrap_content" 
			    android:text="@+id/EditText_write_child_9_5_2_14" 
			    android:layout_width="wrap_content"></EditText>

		</LinearLayout>
		<LinearLayout android:id="@+id/linearLayout5" style="@style/write_9_question_box" android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="wrap_content">
			<TextView style="@style/write_9_question" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/TextView08" android:text="次の健康診査までの記録"></TextView>
			<HorizontalScrollView android:id="@+id/horizontalScrollView1" android:layout_width="wrap_content" android:layout_height="wrap_content">
				<LinearLayout android:id="@+id/linearLayout8" android:layout_width="fill_parent" android:layout_height="fill_parent" android:orientation="horizontal">
					<TableLayout android:id="@+id/tableLayout1" android:layout_width="wrap_content" android:layout_height="fill_parent">
						<TableRow android:id="@+id/tableRow1" android:layout_width="wrap_content" android:layout_height="wrap_content">
							<TextView style="@style/write_9_question" android:layout_height="wrap_content" android:id="@+id/TextView12" android:layout_width="wrap_content" android:text="年月日"></TextView>
							<TextView style="@style/write_9_question" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/TextView13" android:text="年齢"></TextView>
							<TextView style="@style/write_9_question" android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/TextView14" android:text="体重"></TextView>
							<TextView style="@style/write_9_question" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/TextView16" android:text="身長"></TextView>
							<TextView style="@style/write_9_question" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/TextView17" android:text="指導事項"></TextView>
							<TextView style="@style/write_9_question" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/TextView15" android:text="施設名又は担当者名"></TextView>
						</TableRow>
						<TableRow android:layout_width="wrap_content" android:id="@+id/tableRow2" android:layout_height="wrap_content">
							<LinearLayout android:id="@+id/LinearLayout01" android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
								
								<EditText android:id="@+id/EditText_write_child_9_5_2_15" 
								    android:layout_height="wrap_content" 
								    android:layout_width="wrap_content" 
								    android:text="@+id/EditText_write_child_9_5_2_15"></EditText>
								
								
								<TextView style="@style/write_9_question" android:layout_height="wrap_content" android:text="@string/write_year" android:id="@+id/TextView09" android:layout_width="wrap_content"></TextView>
								
								<EditText android:id="@+id/EditText_write_child_9_5_2_16" 
								    android:layout_height="wrap_content" 
								    android:text="@+id/EditText_write_child_9_5_2_16" 
								    android:layout_width="wrap_content"></EditText>
								
								
								<TextView style="@style/write_9_question" android:layout_height="wrap_content" android:text="@string/write_month" android:id="@+id/TextView11" android:layout_width="wrap_content"></TextView>
								
								<EditText android:id="@+id/EditText_write_child_9_5_2_17" 
								    android:layout_height="wrap_content" 
								    android:text="@+id/EditText_write_child_9_5_2_17" 
								    android:layout_width="wrap_content"></EditText>
								
								<TextView style="@style/write_9_question" android:layout_height="wrap_content" android:text="日" android:id="@+id/TextView10" android:layout_width="wrap_content"></TextView>
							</LinearLayout>
							<LinearLayout android:id="@+id/LinearLayout02" android:layout_width="wrap_content" android:orientation="horizontal" android:layout_height="wrap_content">
								
								<EditText android:id="@+id/EditText_write_child_9_5_2_18" 
								    android:layout_height="wrap_content" 
								    android:text="@+id/EditText_write_child_9_5_2_18" 
								    android:layout_width="wrap_content"></EditText>
								
								
								<TextView style="@style/write_9_question" android:layout_height="wrap_content" android:id="@+id/TextView20" android:layout_width="wrap_content" android:text="歳"></TextView>
							</LinearLayout>
							<LinearLayout android:id="@+id/LinearLayout03" android:layout_width="wrap_content" android:orientation="horizontal" android:layout_height="wrap_content">
								
								<EditText android:id="@+id/EditText_write_child_9_5_2_19" 
								    android:layout_height="wrap_content" 
								    android:text="@+id/EditText_write_child_9_5_2_19" 
								    android:layout_width="wrap_content"></EditText>
								
								<TextView style="@style/write_9_question" android:layout_height="wrap_content" android:id="@+id/TextView18" android:layout_width="wrap_content" android:text="kg"></TextView>
							</LinearLayout>
							<LinearLayout android:id="@+id/LinearLayout04" android:layout_width="wrap_content" android:orientation="horizontal" android:layout_height="wrap_content">
								
								<EditText android:id="@+id/EditText_write_child_9_5_2_20" 
								    android:layout_height="wrap_content" 
								    android:layout_width="wrap_content" 
								    android:text="@+id/EditText_write_child_9_5_2_20"></EditText>
								
								<TextView style="@style/write_9_question" android:layout_height="wrap_content" android:id="@+id/TextView19" android:layout_width="wrap_content" android:text="cm"></TextView>
							</LinearLayout>
							
							<EditText android:id="@+id/EditText_write_child_9_5_2_21" 
							    android:layout_height="wrap_content" 
							    android:text="@+id/EditText_write_child_9_5_2_21" 
							    android:layout_width="wrap_content"></EditText>
							
							
							<EditText android:id="@+id/EditText_write_child_9_5_2_22" 
							    android:layout_height="wrap_content" 
							    android:layout_width="wrap_content" 
							    android:text="@+id/EditText_write_child_9_5_2_22"></EditText>
						</TableRow>
					</TableLayout>
				</LinearLayout>
