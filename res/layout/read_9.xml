<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content" android:orientation="vertical" android:background="@drawable/background_pink">

     <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="35dp"
        android:gravity="center" >


        <Button
            android:id="@+id/read_button_9_back"
            style="@style/button_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/ButtonBack" />



        <Button
            android:id="@+id/read_button_9_mneu"
            style="@style/button_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/ButtonMneu" >

        </Button>



        <Button
            android:id="@+id/read_button_9_next"
            style="@style/button_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            
            android:text="@string/ButtonNext" >

        </Button>
        >

    </LinearLayout>
    
    
<ScrollView
	android:id="@+id/ScrollView01"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content">

<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content" android:orientation="vertical">

	<TextView
		android:text="@string/read_9_title"
		android:id="@+id/read_9_title"
		style="@style/read_x_title" />

	<TextView
		 android:text="@string/read_9_description"
		 android:id="@+id/read_9_description"
		 style="@style/read_x_description"/>

	<TextView
		android:text="@string/read_9_1_title"
		android:id="@+id/read_9_1_title"
		style="@style/read_x_x_title" />

	<TextView
		android:text="@string/read_9_1_description"
		android:id="@+id/read_9_1_description"
		style="@style/read_x_x_description" />

	<TextView
		android:text="@string/read_9_1_footnote"
		android:id="@+id/read_9_1_footnote"
		style="@style/read_x_x_footnote" />

	<TextView
		android:text="@string/read_9_2_title"
		android:id="@+id/read_9_2_title"
		style="@style/read_x_x_title" />

	<TextView
		android:text="@string/read_9_2_description"
		android:id="@+id/read_9_2_description"
		style="@style/read_x_x_description" />

<TableLayout android:id="@+id/TableLayout01" android:layout_width="wrap_content" android:layout_height="wrap_content"
	android:collapseColumns="*" android:shrinkColumns="*">
	<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content" android:layout_height="wrap_content">
		<TextView android:text="@string/read_9_2_item1no1" android:id="@+id/read_9_2_item1no1"  style="@style/read_x_x_description" />
		<TextView android:text="@string/read_9_2_item1no2" android:id="@+id/read_9_2_item1no2"  style="@style/read_x_x_description" />
	</TableRow>
	<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content" android:layout_height="wrap_content">
		<TextView android:text="@string/read_9_2_item2no1" android:id="@+id/read_9_2_item2no1"  style="@style/read_x_x_description" />
		<TextView android:text="@string/read_9_2_item2no2" android:id="@+id/read_9_2_item2no2"  style="@style/read_x_x_description" />
	</TableRow>
	<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content" android:layout_height="wrap_content">
		<TextView android:text="@string/read_9_2_item3no1" android:id="@+id/read_9_2_item3no1"  style="@style/read_x_x_description" />
		<TextView android:text="@string/read_9_2_item3no2" android:id="@+id/read_9_2_item3no2"  style="@style/read_x_x_description" />
	</TableRow>
	<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content" android:layout_height="wrap_content">
		<TextView android:text="@string/read_9_2_item4no1" android:id="@+id/read_9_2_item4no1"  style="@style/read_x_x_description" />
		<TextView android:text="@string/read_9_2_item4no2" android:id="@+id/read_9_2_item4no2"  style="@style/read_x_x_description" />
	</TableRow>
	<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content" android:layout_height="wrap_content">
		<TextView android:text="@string/read_9_2_item5no1" android:id="@+id/read_9_2_item5no1"  style="@style/read_x_x_description" />
		<TextView android:text="@string/read_9_2_item5no2" android:id="@+id/read_9_2_item5no2"  style="@style/read_x_x_description" />
	</TableRow>
</TableLayout>

</LinearLayout>
</ScrollView>
</LinearLayout>