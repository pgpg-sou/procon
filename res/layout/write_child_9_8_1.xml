                  <TextView style="@style/blackOnly"  
                   android:id="@+id/write_child_9_8_09" 
                   android:layout_height="wrap_content" 
                   android:layout_width="wrap_content" 
                   android:text="----"></TextView>
		</LinearLayout>




		<!-- 質問４ -->
		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			style="@style/write_9_question_box">

			<TextView
				android:text="@string/write_9_8_q4"
				android:id="@+id/write_9_8_q4"
				style="@style/write_9_question"/>
			
			<!-- テレビや大人の身振りのまねをしますか -->
                  <TextView style="@style/blackOnly"  
                   android:id="@+id/write_child_9_8_10" 
                   android:layout_height="wrap_content" 
                   android:layout_width="wrap_content" 
                   android:text="----"></TextView>		</LinearLayout>




		<!-- 質問５ -->
		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			style="@style/write_9_question_box">

			<TextView
				android:text="@string/write_9_8_q5"
				android:id="@+id/write_9_8_q5"
				style="@style/write_9_question"/>
			<!-- 2言文などをいいますか -->
                  <TextView style="@style/blackOnly"  
                   android:id="@+id/write_child_9_8_11" 
                   android:layout_height="wrap_content" 
                   android:layout_width="wrap_content" 
                   android:text="----"></TextView>
		</LinearLayout>



		<!-- 質問６ -->
		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			style="@style/write_9_question_box">

			<TextView
				android:text="@string/write_9_8_q6"
				android:id="@+id/write_9_8_q6"
				style="@style/write_9_question"/>
