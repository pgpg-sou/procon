                  <TextView style="@style/blackOnly"  
                   android:id="@+id/write_child_9_8_12" 
                   android:layout_height="wrap_content" 
                   android:layout_width="wrap_content" 
                   android:text="----"></TextView>
		</LinearLayout>



		<!-- 質問７ -->
		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			style="@style/write_9_question_box">

			<TextView
				android:text="@string/write_9_8_q7"
				android:id="@+id/write_9_8_q7"
				style="@style/write_9_question"/>
			<!-- 歯磨きの練習をはじめていますか -->
                  <TextView style="@style/blackOnly"  
                   android:id="@+id/write_child_9_8_13" 
                   android:layout_height="wrap_content" 
                   android:layout_width="wrap_content" 
                   android:text="----"></TextView>
		</LinearLayout>



		<!-- 質問８ -->
		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			style="@style/write_9_question_box">

			<TextView
				android:text="@string/write_9_8_q8"
				android:id="@+id/write_9_8_q8"
				style="@style/write_9_question"/>
			<!-- 保護者が歯の仕上げみがきをしてあげていますか -->
                  <TextView style="@style/blackOnly"  
                   android:id="@+id/write_child_9_8_14" 
                   android:layout_height="wrap_content" 
                   android:layout_width="wrap_content" 
                   android:text="----"></TextView>
		</LinearLayout>



		<!-- 質問９ -->
		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			style="@style/write_9_question_box">

			<TextView
				android:text="@string/write_9_8_q9_1"
				android:id="@+id/write_9_8_q9_1"
				style="@style/write_9_question"/>

			<TextView
				android:text="@string/write_9_8_q9_2"
				android:id="@+id/write_9_8_q9_2"
				style="@style/write_9_question"/>
			<!-- どんな遊びが好きですか -->
                  <TextView style="@style/blackOnly"  
                   android:id="@+id/write_child_9_8_15" 
                   android:layout_height="wrap_content" 
                   android:layout_width="wrap_content" 
                   android:text="----"></TextView>
		</LinearLayout>



		<!-- 質問１０ -->
		<LinearLayout
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			style="@style/write_9_question_box">

			<TextView
				android:text="@string/write_9_8_q10"
				android:id="@+id/write_9_8_q10"
				style="@style/write_9_question"/>
			<!-- 育児の心配，かかった病気，感想などを自由に記入しましょう -->
                  <TextView style="@style/blackOnly"  
                   android:id="@+id/write_child_9_8_16" 
                   android:layout_height="wrap_content" 
                   android:layout_width="wrap_content" 
                   android:text="----"></TextView>
		</LinearLayout>

	</LinearLayout>

</ScrollView>

</LinearLayout>

